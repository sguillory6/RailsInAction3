<!DOCTYPE html>
<html>
<head>
  <title> <%= title %> </title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<%= notice %>
<%= alert %>

<h1><%= link_to "Ticketee", root_path %></h1>

<% if !user_signed_in? %>
    <%= link_to "Sign up", new_user_registration_path %>
<% else %>]
    Signed in as <%= current_user.email %>
<% end %>

<%= yield %>

</body>
</html>
